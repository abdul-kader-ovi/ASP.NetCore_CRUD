@model ClassRoutineManagement.ViewModels.ClassRoutineCreateViewModel

<h4>Class Routine</h4>

<div class="modal fade" id="addClassRoutine">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title" id="addRoutineLabel">Detail Routine</h4>
                <button type="button" class="close" data-dismiss="modal">
                    <span>x</span>
                </button>
            </div>

            <div class="modal-body">
                <form asp-action="Details">
                    <div class="form-group">
                        <label asp-for="Id" class="control-label">asp-for="@Model.Id"</label>
                    </div>
                    <div class="form-group">

                        <label asp-for="DayOfWeek" class="control-label">asp-for="@Model.DayOfWeek"</label>
                    </div>
                    <div class="form-group">
                        <label asp-for="StartTime" class="control-label">@Model.StartTime</label>

                    </div>
                    <div class="form-group">
                        <label asp-for="EndTime" class="control-label">@Model.EndTime</label>

                    </div>

                    <div class="form-group">
                        <label asp-for="ClassesId" class="control-label">@Model.ClassesId</label>

                    </div>
                    <div class="form-group">
                        <label asp-for="SubjectName" class="control-label">@Model.SubjectName</label>

                    </div>
                    <div class="form-group">
                        <label asp-for="ClassRoomId" class="control-label">@Model.SubjectName</label>


                    </div>
                </form>
            </div>

        </div>
    </div>
</div>
